@model WebApplication1.Models.Project

<link rel="stylesheet" href="~/css/PageCard/PageCard.css" />
<link rel="stylesheet" href="~/css/Projects/ProjectsStyle.css" />

<script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.2.2/pdf.js"></script>
<script>
    window.pdfjsLib.GlobalWorkerOptions.workerSrc = 'https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.2.2/pdf.worker.js';
</script>

<div class="CardContainer" >
    <div class="CardSection"style="overflow:hidden;border-color:black;">
        <embed src="~/Projects/@Model.FilePath" type="application/pdf" width="100%" height="100%">
    </div>
</div>

<script src="/js/PDFObject-master/pdfobject.min.js"></script>
<script src="/js/PDFObject-master/pdfobject.js"></script>


